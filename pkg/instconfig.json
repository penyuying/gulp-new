    },{
        "ifmin":true,
        "src":["{#installSrc#}package1"],
        "dest":"{#subroot#}",
        "newFileName":"package.json"
    },{
        "ifmin":true,
        "src":["{#installSrc#}package1"],
        "dest":"{#subroot_svn#}",
        "newFileName":"package.json"
    },{
        "ifmin":true,
        "src":["{#installSrc#}webAppConfig"],
        "dest":"{#gulpRoot#}"
    },{
        "ifmin":true,
        "src":"{#installSrc#}pkg/*",
        "dest":"{#subroot#}pkg/"
    },{
        "ifmin":true,
        "src":["{#installSrc#}package"],
        "dest":"{#gulpRoot_svn#}"
    },{
        "ifmin":true,
        "src":["{#installSrc#}webAppConfig1"],
        "dest":"{#gulpRoot_svn#}",
        "newFileName":"webAppConfig.json"
    },{
        "ifmin":true,
        "src":["{#installSrc#}pkg/*","!{#installSrc#}pkg/copysvn.*"],
        "dest":"{#subroot_svn#}pkg/"
    }],
    "evalConfig":{},
