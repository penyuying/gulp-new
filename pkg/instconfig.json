    "gulpRoot_svn":"{#subroot_svn#}gulp/",
    "serverPath": "",

    "jsDoc3Temp":"./temp/jsDoc/instconfig/",
    "jsDoc3Dir":"./Doc/{#name#}/",
    "ifJsDoc":true,

    "browser": "",
    "copyFile": [{
        "src":"{#githubsrc#}fonteditor/save/KidMadeTo/KidMadeTo.*",
        "dest":"{#subDevelop#}/sass/fonts/KidMadeTo/"
    },{
        "src":"{#githubsrc#}fonteditor/save/KidMadeTo3.0/KidMadeTo.*",
        "dest":"{#htmlDevelop#}/sass/fonts/KidMadeTo/"
    }],
    "copyDstDir": "",
    "jsonFile": [{
