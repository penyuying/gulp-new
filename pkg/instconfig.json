{
    "rem": "/*处理过后的json的文件和目录存放的目录*/",
    "name": "instconfig",
    "userName": "penyu",
    "version": "1.0.0",
    "ifmin": false,
    "ifminhtml": false,
    "gzipIf": false,
    "changIf": false,
    "srcRev": false,
    "suffix": "",
    "srcPath": "../",
    "installSrc":"gulpInst/instconfig/",
    "publicPath": "{#srcPath#}publicTool/public/",
    "subroot":"e:/webapp/",
