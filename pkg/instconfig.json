        "ifEval":false,
        "ifmin":true,
        "src":"{#installSrc#}pcss"
    },{
        "src":"{#installSrc#}gulpfile",
        "dest":"{#gulpRoot_svn#}"
    },{
        "ifEval":false,
        "ifmin":true,
        "src":"{#installSrc#}pcss",
