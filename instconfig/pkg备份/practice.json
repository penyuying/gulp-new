{
    "name": "practice",
    "userName": "penyu",
    "version": "1.0.0",

    "ifmin": true,
    "ifEval":false,
    "ifJsDoc":false,
    "ifEncrypt":false,
    "encryptConfig":{},
    "gzipIf": false,
    "srcRev":true,
    "suffix": "",
	"injectIf":false,
	"bannerIf":false,
	"changIf":false,
	"jsAnonymous":true,

    "webPath": "{#webTestPath#}",
    "srcPath": "{#webappDir#}develop/practice/",
    "destPath": "{#webPath#}/practice/",
    "revDestPath":"{#tempPath#}rev/practice/",

    "jsDoc3Temp":"{#tempPath#}jsDoc/practice/",
    "jsDoc3Dir":"{#webPath#}Doc/{#name#}/",
    "isTest":false,
    "testConfig":{
        "srcPath":[
            "src/**/*.js",
            "test-lib/**/*.js",
            "test/**/*.js"
        ],
        "testConfigFile":"{#srcPath#}karma.conf.js",
        "singleRun":false
    },
    "destRoot":"pay/",

    "host": "127.0.0.1",
    "port": "8094",
    "serverPath": "{#webPath#}/practice/",
    "browser": "chrome",
	"anonymous":true,

    "copyFile": [{
        "psrc": "js/jquery/jquery-1.11.3.min.js",
        "dest":"js/jquery/"
    },{
        "psrc": "js/jquery/plugins/select2/select2-3.5.2/{select2.min.js,select2.css,select2.png,select2-spinner.gif}",
        "dest":"js/jquery/"
    }],
    "copyDstDir": "",


    "jsonFile": [{
        "src":"json/**/*",
        "ifEncrypt":true,
        "encryptConfig":{
            "type":"unicode,des",
            "extname":"ud",
            "password":"penyuying"
        }
    }],
    "jsonDstDir": "json/",

    "imgFile": [{
        "src": "{img/,img/**/*}"
	}, {
        "src": "css/**/*",
        "dest": "css/"
	}, {
        "src": "sass/**/*",
        "dest": "css/"
    }, {
        "src": "compass/**/*",
        "dest": "css/"
	}],
    "imgDstDir": "img/",

    "dirConcatJs": [{
        "jsAnonymous":true,
        "src": "",
        "psrc":"js/_subDir/",
        "dest": "js/"
	}],


    "concatJs":[{
        "jsAnonymous":false,
        "psrc":["js/encrypt/AES/tripledes","js/encrypt/AES/mode-ecb"],
        "fileName":"AES.js",
        "dest": "js/"
    }],
    "concatDstJsFileName":"",

    "tsFile":[{
        "src":"ts/*",
        "fileName":"",
        "dest": "tsJs/"
    }],

    "jsFile": [{
        "src": "js/**/*",
        "dest": "js/",
        "debar": "{_subDir/,_subDir/**/*.*}"
	},{
        "psrc": "js/jquery/plugins/json/jquery.json.min",
        "dest":"js/"
    }],
    "jsDstDir": "js/",


    "sassFile": [{
        "src":"sass/**/*",
        "dest":"css/"
    }],

    "compassFile": [{
        "src":"compass/**/*",
        "dest":"css/"
    }],
    "compassTemp":"{#tempPath#}compass/practice/",
    "compassConfig": {
        "project": "{#srcPath#}/",
        "config_file": "./config.rb",
        "sass": "compass/",
        "image": "compass/img/"
    },

    "concatCss":"",
    "concatDstCssFileName":"",

    "cssFile":[
        {
            "src":"!css/**/*",
            "dest":"css/"
        }
    ],
    "cssDstDir": "css/",
    "ngTplsFile":[{
        "ifEval":false,
        "bannerIf":true,
        "destRoot":"",
        "jsAnonymous":false,
        "src":"ngtpls/**/*",
        "dest":"js/",
        "destPath":"{#srcPath#}",
        "conf":{
            "moduleName": "genTemplates",
            "standalone": false,
            "useStrict": false,
            "prefix":"tpls/",
            "filePath": "templates.js"
        }
    }],
    "ngTplsConf":{
          "moduleName": "genTemplates",
          "standalone": false,
          "useStrict": false,
          "prefix":"",
          "filePath": "templates.js"
    },
    "ngTplsDstDir":"js/",
	"ifminhtml": false,
	"ifminhtmlObj":{
        "removeComments": true,
        "collapseWhitespace": true,
        "conservativeCollapse": true,
        "preserveLineBreaks": false,
        "removeAttributeQuotes":false,
        "removeEmptyAttributes": false,
        "removeEmptyElements": false,
        "removeScriptTypeAttributes": false,
        "collapseBooleanAttributes": false,
        "removeStyleLinkTypeAttributes": false,
        "keepClosingSlash": true,
        "minifyJS": true,
        "minifyCSS":true
    },

    "htmlFile": [{
        "changIf":true,
        "destRoot":"",
        "src":"html/*",
        "debar": "tpls/**/*.*",
        "tpls":"html/tpls/"
    },{
		"changIf":true,
        "destRoot":"",
        "src":"html/webgl/**/*",
        "debar": "tpls/**/*.*",
        "tpls":"html/tpls/",
        "dest":"webgl/"
    }],
    "tplsHtmlFile": "html/tpls/",
    "htmlDstDir":"",
    "injectPath":""
}