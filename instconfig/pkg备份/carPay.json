{
    "name": "carPay",
    "userName": "penyu",
    "version": "1.0.0",


    "ifmin": false,
    "ifEval":true,
    "ifJsDoc":true,
    "gzipIf": false,
    "srcRev": true,
    "suffix": "",
	"injectIf":true,
	"bannerIf":false,
	"changIf":false,
	"jsAnonymous":false,

    "srcPath": "{#webappDir#}develop/carPay/html/",
    "destPath": "{#webPath#}collect/carPay/",
    "revDestPath":"{#tempPath#}rev/carPay/",
	"revType":"part",
    "destRoot":"",

    "jsDoc3Temp":"{#tempPath#}jsDoc/carPay/",
    "jsDoc3Dir":"{#webPath#}Doc/{#name#}/",

    "host": "127.0.0.1",
    "port": "8096",
    "serverPath": "{#webPath#}collect/carPay/",
    "browser": "chrome",
	"anonymous":true,
    "copyFile": [{
            "psrc": "js/jquery/jquery-1.11.3.min.js",
            "dest":"js/jquery/"
		},{
            "psrc": "js/layer.mobile/layer.mobile-v1.6/layer.m/**/*.css",
            "dest":"js/layer/"
		}],
    "copyDstDir": "",


    "jsonFile": "json/{city,proJson}",
    "jsonDstDir": "json/",

    "imgFile": [{
            "src": "{img/,img/**/*}"
    	},{
            "src": ["css/**/*","sass/**/*"],
            "dest": "css/"
	}],
    "imgDstDir": "img/",

    "dirConcatJs": [{
        "src": "",
        "psrc":"",
        "dest": "js/"
	}],
    "concatJs":[{
            "ifmin": false,
            "ifEval":false,
            "jsAnonymous":true,
            "psrc": "js/zeptoJs1.1.6/{zepto,ajax,event,data,touch}",
            "fileName":"zepto.min.js",
            "dest":"js/zepto/"
        },{
            "jsAnonymous":true,
            "psrc":"js/sub_js/switch/{Keyboard,mobileStock,mobileTimeSolt}",
            "src":"js/carPay",
            "fileName":"carPay.js",
            "dest": "js/"
        },{
            "ifmin": false,
            "ifEval":false,
            "jsAnonymous":true,
            "psrc":"js/sub_js/initial/initial_jq",
            "fileName":"initial.js",
            "dest": "js/comm/"
    	},{
            "ifmin": false,
            "ifEval":false,
            "jsAnonymous":true,
            "psrc":"js/sub_js/controls/Radio",
            "fileName":"controls.js",
            "dest": "js/comm/"
    	},{
            "ifmin": false,
            "ifEval":false,
            "jsAnonymous":true,
    		"jsGlobalObj":"window,$",
            "psrc":"js/qrcode/{qrcode,jquery.qrcode,utf16to8}",
            "fileName":"qrcode.js",
            "dest": "js/comm/"
	}],
    "concatDstJsFileName":"",
    "jsFile": [{
            "ifmin": false,
            "ifEval":false,
            "psrc": "!js/comm/comm",
            "dest": "js/",
            "destPath":"{#tempPath#}jsdocTemp/{#name#}/"
    	},{
            "ifmin": false,
            "ifEval":false,
            "psrc": "js/layer.mobile/layer.mobile-v1.6/layer.m/**/*",
            "dest":"js/layer/"
        },{
            "ifmin": false,
            "ifEval":false,
            "psrc": "js/{viewport,pinyin}",
            "dest": "js/comm/"
    	},{
            "ifmin": false,
            "ifEval":false,
            "jsAnonymous":true,
            "jsGlobalObj":"window",
            "psrc": "js/public-obj",
            "dest": "js/comm/"
    	},{
            "ifmin": false,
            "ifEval":false,
            "psrc": "js/encrypt/sha1",
            "dest": "js/comm/"
    	},{
            "ifmin": false,
            "ifEval":false,
            "psrc": "js/location/getCity",
            "dest": "js/"
    	},{
            "src": "js/*",
            "debar": "{js/_subDir/,js/_subDir/**/*.*,js/Psub_file/,js/Psub_file/**/*.*,**/carPay.js}"
    	},{
            "ifmin": false,
            "ifEval":false,
    		"psrc": "js/zeptoJs1.1.6/plugins/json/zepto.json.min",
    		"dest":"js/zepto/"
	}],
    "jsDstDir": "js/",


    "sassFile": [{
        "src":"sass/**/*",
        "dest":"css/"
    }],

    "concatCss":"",
    "concatDstCssFileName":"",

    "cssFile":[{
		"src":"css/**/*",
		"psrc":"css/control"
	}],
    "cssDstDir": "css/",

	"ifminhtml": false,
    "ifminhtmlObj":{
        "removeComments": true,
        "collapseWhitespace": true,
        "conservativeCollapse": true,
        "preserveLineBreaks": false,
        "removeAttributeQuotes":false,
        "removeEmptyAttributes": false,
        "removeEmptyElements": false,
        "removeScriptTypeAttributes": false,
        "collapseBooleanAttributes": false,
        "removeStyleLinkTypeAttributes": false,
        "keepClosingSlash": true,
        "minifyJS": true,
        "minifyCSS":true
    },
    "htmlFile": [{
		"changIf":true,
        "destRoot":"",
        "src":"html/*",
        "tpls":"html/tpls/"
    }],
    "tplsHtmlFile": "html/tpls/",
    "htmlDstDir":"",
    "injectPath":""
}
