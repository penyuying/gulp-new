{
    "name": "collectMobile",
    "userName": "",
    "version": "1.0.0",

    "ifmin": false,
    "gzipIf": false,
    "srcRev": true,
    "suffix": "",
	"injectIf":true,
	"bannerIf":false,
	"changIf":false,
	"jsAnonymous":false,
    "mapIf": false,


    "srcPath": "{#webappDir#}develop/collectMobile/html/",
    "destPath": "{#webPath#}collect/",
    "destTest": "{#webPath#}test/collectMobile/",
    "revDestPath":"{#tempPath#}/rev/collectMobile/",

    "jsDocType":"angular",
    "jsDocLink":"/api",
    "jsDoc3Temp":"{#tempPath#}jsDoc/wapsf/",
    "jsDoc3Dir":"{#webPath#}Doc/{#name#}/",
    "ifJsDoc":true,
    "destRoot":"wapsf/",

    "host": "127.0.0.1",
    "port": "8091",
    "serverPath": "{#webPath#}/wapsf/",
    "browser": "chrome",
	"anonymous":true,

    "copyFile": [{
            "changIf":false,
            "psrc": "sass/global/mixin.scss",
            "dsrc":"sass/",
            "newFileName":"_mixin.scss"
        },{
            "psrc": "js/require/requirejs2.1.11/require.min.js",
            "dest":"js/comm/"
        },{
            "src":"release/fonts/**/*",
            "dest":"js/ionic/fonts/",
            "srcPath":"{#githubsrc#}ionic/"
        },{
            "psrc": "js/layer.mobile/layer.mobile-v1.6/layer.m/**/*.*",
            "dest":"js/layer/"
        }],
    "copyDstDir": "",


    "jsonFile": [{
        "src": "json/QueryChargeResponse",
        "dest": "json/"
	}],
    "jsonDstDir": "json/",

    "imgFile": [{
            "src": "{img/,img/**/*}"
    	},{
            "src": "sass/**/*",
            "dest": "css/"
    	},{
            "src": "css/**/*",
            "dest": "css/"
    	},{
    		"destRoot":"",
            "psrc": "img/wxpic/**/*",
            "dest": "wxpic/"
	}],
    "imgDstDir": "img/",

    "dirConcatJs":"",

    "concatJs":[{
            "jsAnonymous":true,
            "psrc": "js/zeptoJs1.1.6/{zepto,ajax,event,data}",
            "fileName":"zepto.min.js",
            "dest":"js/zepto/"
        },{
            "jsAnonymous":true,
            "jsGlobalObj":"window",
            "psrc": "js/public-obj",
            "fileName":"public-obj.js",
            "dest": "js/comm/"
        },{
            "jsAnonymous":true,
            "jsGlobalObj":"window,angular",
            "src": "js/collect/_subJs/collect/**/*",
            "psrc":"js/collect/_subJs/collect/{factory,filter}",
            "fileName":"collect.js",
            "dest": "js/collect/",
            "publicPath": "{#webappDir#}develop/collect/html/"
        },{
            "ifJsDoc":false,
            "jsAnonymous":true,
            "jsGlobalObj":"window",
            "src": [
                "release/js/angular/angular",
                "release/js/angular/angular-animate",
                "release/js/angular/angular-sanitize",
                "release/js/angular-ui/angular-ui-router",

                "js/ionic",
                "js/utils/delegateService",
                "js/utils/dom",
                "js/utils/events",
                "js/utils/gestures",
                "js/utils/platform",
                "js/utils/poly",
                "js/utils/tap",
                "js/utils/activator",
                "js/utils/utils",
                "js/utils/list",
                "js/utils/keyboard",
                "js/utils/viewport",

                "js/views/view",
                "js/views/scrollView",
                "js/views/scrollViewNative",
                "js/views/listView",
                "js/views/modalView",
                "js/views/sideMenuView",
                "js/views/sliderView",
                "js/views/toggleView",

                "js/angular/*",
                "js/angular/service/**/*",
                "js/angular/controller/**/*",
                "js/angular/directive/**/*"
            ],

            "dest":"js/ionic/",
            "debar": "{slide*.*,side*.*,menu*.*,tab*.*,checkbox*.*,popover*.*,spinner*.*,modal*.*,popup*.*,keyboard*.*}",
            "fileName":"ionic.bundle.min.js",
            "srcPath":"{#githubsrc#}ionic/"
        }],


    "concatDstJsFileName":"",

    "jsFile": [{
            "psrc": "js/encrypt/sha1",
            "dest": "js/comm/"
        },{
            "psrc": "js/location/getCity",
            "dest": "js/comm/"
        },{
            "psrc": "js/{viewport,pinyin}",
            "dest": "js/comm/"
        },{
            "src": "!ionic_1.0.0/release/js/ionic.bundle.min",
            "dest":"js/ionic/",
            "srcPath":"{#bowersrc#}"
        },{
            "psrc": "!js/angular/plugins/ui_router/angular-ui-router",
            "dest":"js/angular/"
        },{
            "psrc": "js/zeptoJs1.1.6/plugins/json/zepto.json.min",
            "dest":"js/zepto/"
        },{
            "jsAnonymous":true,
            "jsGlobalObj":"window",
            "src": "js/*",
            "dest": "js/",
            "debar": "{js/_subDir/,js/_subDir/**/*.*}"
     }],
    "jsDstDir": "js/",


    "sassFile": [{
            "src":"sass/**/*",
            "dest":"css/"
        },{
            "src":"scss/*",
            "dest":"js/ionic/css/",
            "srcPath":"{#githubsrc#}ionic/"
    }],

    "concatCss":"",
    "concatDstCssFileName":"",

    "cssFile":"css/**/*",
    "cssDstDir": "css/",

	"ifminhtml": false,
	"ifminhtmlObj":{
        "removeComments": true,
        "collapseWhitespace": true,
        "conservativeCollapse": true,
        "preserveLineBreaks": true,
        "removeAttributeQuotes":false,
        "removeEmptyAttributes": false,
        "removeEmptyElements": false,
        "removeScriptTypeAttributes": false,
        "collapseBooleanAttributes": false,
        "removeStyleLinkTypeAttributes": false,
        "keepClosingSlash": true,
        "minifyJS": true,
        "minifyCSS":true
    },

    "templateFile":[{
        "injectIf":false,
        "src":"tpls/**/*",
        "dest":"tpls/"
    }],
    "templateDstDir":"tpls/",

    "htmlFile": [{
		"changIf":true,
        "src":"html/**/*",
		"destRoot":"",
		"dest":"wapsf/"
    }],

    "tplsHtmlFile": "html/tpls/",
    "htmlDstDir":"",
    "injectPath":""
}