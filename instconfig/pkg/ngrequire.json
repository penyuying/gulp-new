{
	"name": "ngrquire",
    "userName": "penyu",
    "version": "1.0.0",

    "ifmin": true,
    "gzipIf": false,
    "srcRev": false,
    "suffix": "",
	"injectIf":true,
	"changIf":true,
    "srcPath": "{#webappDir#}develop/ngrquire/src/",
    "destPath": "{#webPath#}ngrquire/",
    "revDestPath":"{#tempPath#}rev/ngrquire/",
    "destRoot":"",

    "jsDoc3Temp":"{#tempPath#}jsDoc/ngrquire/",
    "jsDoc3Dir":"{#webPath#}Doc/{#name#}/",
    "ifJsDoc":true,

    "host": "127.0.0.1",
    "port": "8098",
    "serverPath": "{#webPath#}ngrquire/",
    "browser": "chrome",

    "copyFile": [{
            "src": "ocLazyLoad.require.min.js",
            "dest":"js/",
            "srcPath":"E:/github/ocLazyLoad/dist/"
		}],
    "copyDstDir": "",

    "jsonFile": [],
    "jsonDstDir": "json/",

	"imgFile": [{
        "src": "img/**/*",
		"dest": "img/"
	}, {
        "src": "css/**/*",
        "dest": "css/"
	}, {
        "src": "sass/**/*",
        "dest": "css/"
	}],
    "imgDstDir": "img/",
	"concatJs":[],
	"concatDstJsFileName":"",

	"jsFile": [],
    "jsDstDir": "js/",

	"sassFile": [{
        "src":"sass/**/*",
        "dest":"css/"
    }],

	"cssFile":"css/**/*",
    "cssDstDir": "css/",


	"ifminhtml": false,
	"ifminhtmlObj":{
        "removeComments": true,
        "collapseWhitespace": true,
        "conservativeCollapse": true,
        "preserveLineBreaks": true,
        "removeAttributeQuotes":false,
        "removeEmptyAttributes": false,
        "removeEmptyElements": false,
        "removeScriptTypeAttributes": false,
        "collapseBooleanAttributes": false,
        "removeStyleLinkTypeAttributes": false,
        "keepClosingSlash": true,
        "minifyJS": true,
        "minifyCSS":true
    },
	"htmlFile": [{
        "src":"html/**/*",
        "debar": "tpls/**/*.*",
        "tpls":"html/tpls/"
    }],

    "tplsHtmlFile": "html/tpls/",
    "htmlDstDir":"",

	"injectPath":""
}