{
    "name": "carPay",
    "userName": "penyu",
    "version": "1.0.0",

    "ifmin": false,
    "gzipIf": false,
    "srcRev": true,
    "suffix": "",
	"injectIf":true,
	"bannerIf":false,
	"changIf":false,
	"jsAnonymous":false,

    "srcPath": "{#webappDir#}develop/carPay/html/",
    "destPath": "{#webPath#}collect/carPay/",
    "revDestPath":"{#tempPath#}rev/carPay/",
	"revType":"part",
    "destRoot":"",

    "jsDoc3Temp":"{#tempPath#}jsDoc/carPay/",
    "jsDoc3Dir":"{#webPath#}Doc/{#name#}/",
    "ifJsDoc":true,

    "host": "127.0.0.1",
    "port": "8096",
    "serverPath": "{#webPath#}collect/carPay/",
    "browser": "chrome",
	"anonymous":true,

    "copyFile": [{
            "psrc": "js/jquery/jquery-1.11.3.min.js",
            "dest":"js/jquery/"
		},{
            "psrc": "js/zeptoJs1.1.6/zepto.min.js",
            "dest":"js/zepto/"
		},{
            "psrc": "js/layer.mobile/layer.mobile-v1.6/layer.m/**/*.*",
            "dest":"js/layer/"
		}],
    "copyDstDir": "",
	
	
    "jsonFile": "json/{city,proJson}",
    "jsonDstDir": "json/",

    "imgFile": [{
        "src": "{img/,img/**/*}"
	},{
        "src": ["css/**/*","sass/**/*"],
        "dest": "css/"
	}],
    "imgDstDir": "img/",

    "dirConcatJs": [{
        "src": "",
        "psrc":"",
        "dest": "js/"
	}],
    "concatJs":[{
        "jsAnonymous":true,
        "psrc":"js/sub_js/switch/Keyboard",
        "src":"js/carPay",
        "fileName":"carPay.js",
        "dest": "js/"
    },{
        "jsAnonymous":true,
        "psrc":"js/sub_js/initial/initial_jq",
        "fileName":"initial.js",
        "dest": "js/comm/"
	},{
        "jsAnonymous":true,
        "psrc":"js/sub_js/controls/Radio",
        "fileName":"controls.js",
        "dest": "js/comm/"
	},{
        "jsAnonymous":true,
		"jsGlobalObj":"window,$",
        "psrc":"js/qrcode/{qrcode,jquery.qrcode,utf16to8}",
        "fileName":"qrcode.js",
        "dest": "js/comm/"
	},{
        "jsAnonymous":true,
		"jsGlobalObj":"window,$,YW",
        "psrc":"js/sub_js/createListNode/createListNode",
        "src":"js/Psub_file/community",
        "fileName":"community.js",
        "dest": "js/"
	}],
    "concatDstJsFileName":"",
    "jsFile": [{
        "psrc": "js/comm/comm",
        "dest": "js/",
        "destPath":"{#tempPath#}jsdocTemp/{#name#}/"
	},{
        "psrc": "js/{viewport,pinyin}",
        "dest": "js/comm/"
	},{
        "jsAnonymous":true,
        "jsGlobalObj":"window",
        "psrc": "js/public-obj",
        "dest": "js/comm/"
	},{
        "psrc": "js/encrypt/sha1",
        "dest": "js/comm/"
	},{
        "psrc": "js/location/getCity",
        "dest": "js/"
	},{
        "src": "js/*",
        "debar": "{js/_subDir/,js/_subDir/**/*.*,js/Psub_file/,js/Psub_file/**/*.*,**/carPay.js}"
	},{
		"psrc": "js/zeptoJs1.1.6/plugins/json/zepto.json.min",
		"dest":"js/zepto/"
	}],
    "jsDstDir": "js/",
    

    "sassFile": [{
        "src":"sass/**/*",
        "dest":"css/"
    }],
    
    "concatCss":"",
    "concatDstCssFileName":"",
    
    "cssFile":[{
		"src":"css/**/*",
		"psrc":"css/control"
	}],
    "cssDstDir": "css/",

	"ifminhtml": false,
    "ifminhtmlObj":{
        "removeComments": true,
        "collapseWhitespace": true,
        "conservativeCollapse": true,
        "preserveLineBreaks": true,
        "removeAttributeQuotes":false,
        "removeEmptyAttributes": false,
        "removeEmptyElements": false,
        "removeScriptTypeAttributes": false,
        "collapseBooleanAttributes": false,
        "removeStyleLinkTypeAttributes": false,
        "keepClosingSlash": true,
        "minifyJS": true,
        "minifyCSS":true
    },
    "htmlFile": [{
		"changIf":true,
        "destRoot":"",
        "src":"html/*",
        "tpls":"html/tpls/"
    }],
    "tplsHtmlFile": "html/tpls/",
    "htmlDstDir":"",
    "injectPath":""
}