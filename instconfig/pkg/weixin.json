{
    "name": "微信小程序",
    "userName": "kid",
    "version": "v0.0.1",


    "connectStart": true,

    "ifbabel":false,
    "babelEnvConfig":{
        "targets": {
            "browsers": ["last 5 versions"]
        },
        "spec":false,
        "loose":true,
        "modules":false,
        "useBuiltIns": true,
        "debug": false
    },

    "isJshint":true,
    "isEslint":true,
    "isTslint":false,
    "ifmin": false,
    "ifEval":false,
    "ifJsDoc":false,
    "gzipIf": false,
    "srcRev": false,
    "suffix": "",
    "injectIf":true,
    "bannerIf":true,
    "changIf":false,
    "jsAnonymous":false,
    "mapIf": false,

    "srcPath": "{#webappDir#}src/",
    "destPath": "{#webPath#}build/",
    "destRoot":"",
    "destTest": "{#webPath#}test/",
    "revDestPath":"{#tempPath#}/rev/build/",

    "jsDocType":"angular",
    "jsDocLink":"/api",
    "jsDoc3Temp":"{#tempPath#}jsDoc/build/",
    "jsDoc3Dir":"{#webPath#}Doc/{#name#}/",
    "isTest":false,
    "testConfig":{
        "srcPath":[
            "../test/**/*.js",
            "*.js"
        ],
        "testConfigFile":"{#destTest#}sf/test/config/karma.conf.js",
        "singleRun":false
    },

    "host": "127.0.0.1",
    "port": "8091",
    "serverPath": "{#webPath#}/build/",
    "browserbak": "chrome,firefox",
    "browser": "chrome",
    "anonymous":true,

    "copyFile": [{
        "src":"**/*.wxss",
        "dest":""
    },{
        "src":"**/*.wxs",
        "dest":""
    }],
    "copyDstDir": "",


    "jsonFile": [{
        "src":"**/*",
        "dest":""
    }],
    "jsonDstDir": "",

    "imgFile": [{
        "src":"**/*",
        "dest":""
    }],
    "imgDstDir": "",

    "dirConcatJs": "",

    "concatJs":[],
    "concatDstJsFileName":"",

    "tsFile":[{
        "bannerIf":true,
        "ifEval":false,
        "src":["../tsconfig.json","**/*"],
        "dest":""
    }],
    "tslintFile":"**/*.ts",
    "tsConfFile":"../tsconfig.json",

    "jsFile": [{
        "src":"**/*",
        "dest":""
    }],
    "jsDstDir": "",
    "NODE_ENV":"development",
    "webpackFile":"",
    "webpackHtmlTpls":"",
    "webpackDstDir":"",
    "webpackConfig":"",

    "ngTplsFile":"",
    "ngTplsConf":{},
    "ngTplsDstDir":"",

    "sassFile":[{
        "src":"**/*",
        "extname":".wxss",
        "dest":""
    }],


    "compassFile": "",
    "compassTemp":"",
    "compassConfig": "",


    "concatCss":"",
    "concatDstCssFileName":"concatCss.css",

    "cssFile":[{
        "src":"**/*",
        "extname":".wxss",
        "dest":""
    }],
    "cssDstDir": "",

    "ifminhtml": false,
    "ifminhtmlObj":{
        "caseSensitive":true,
        "removeComments": true,
        "collapseWhitespace": true,
        "conservativeCollapse": true,
        "preserveLineBreaks": false,
        "removeAttributeQuotes":false,
        "removeEmptyAttributes": false,
        "removeEmptyElements": false,
        "removeScriptTypeAttributes": false,
        "collapseBooleanAttributes": false,
        "collapsewhitespace":false,
        "removeStyleLinkTypeAttributes": false,
        "keepClosingSlash": true,
        "minifyJS": true,
        "minifyCSS":true
    },

    "templateFile":[{
        "src":"**/*",
        "extname":".wxml",
        "dest":""
    }],
    "templateDstDir":"",

    "htmlFile": "",

    "tplsHtmlFile": "_tpls/",
    "htmlDstDir":"",
    "injectPath":""
}