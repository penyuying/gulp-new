{
    "ifbabel":true,
	"ifmin": false,
	"mapIf": false,
	"bannerIf": false,
	"connectStart": true,
    "taskWatch":true,
    "changIf":true,
    "compassConfig": {
        "project": "{#srcPath#}",
        "css":"{#compassTemp#}/css/",
        "config_file": "./config.rb",
        "sass": "sass/",
        "image": "sass/img/",
        "relative":true,
        "sourcemap":false,
        "generated_images_path":"{#compassTemp#}css/img/",
        "http_path":"./",
        "import_path":""
    },
    "NODE_ENV":"production",
    "vueConfig":{
        "configFile":"{#webappDir#}build/webpack.prod.conf.js",
        "srcPath":"{#webappDir#}/vuesrc/**/*"
    }
}